    "opn": "^5.1.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.0.4",
    "@types/express": "^4.11.0",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.0.26",
    "@types/sinon": "^2.3.3",
    "@types/xml2js": "^0.4.2",
    "chai": "^4.1.2",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.2"
  }
